<h1>Listing of All Active Users</h1>


<ul id="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.username, user_path(user) %> ( User# <%= user.id %>)
    </li>
    
  
  <% end %>
</ul>

<h1>List of all Events</h1>

<ul id="events">
  <% @events.each do |event| %>
    <li>
      <%= event.title %>   -- Created by User: <%= event.user_id %>
    </li>

  <% end %>
</ul>

<%= link_to "<< Create a New User", new_user_path %>
<%= link_to "<< Back to All Events", events_path %>
